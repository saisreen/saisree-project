<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
        
<beans>


<bean id ="emp" class="com.ex.Employee">


<constructor-arg value ="1"></constructor-arg>
<constructor-arg value ="sai"></constructor-arg>
<constructor-arg value ="1000"></constructor-arg>
<constructor-arg ref="empBean"></constructor-arg>
</bean>


<bean id ="empBean" class="com.ex.Address">


<constructor-arg value ="2"></constructor-arg>
<constructor-arg value ="Dallas"></constructor-arg>
<constructor-arg value ="Texas"></constructor-arg>
<constructor-arg value ="USA"></constructor-arg>

</bean>



</beans>